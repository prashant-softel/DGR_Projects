    function GetSPVY() {
        let state_id = "";
        if($('#stateY').val())
        {
            $('select#stateY option:selected').each(function () {
                state_id += "'"+$(this).val() + "',";
            });
            state_id = state_id != "" ? state_id.slice(0, -1) : state_id; 
        }
        $.ajax({
            type: "GET",
            url: '@Url.Action("GetSPVList", "WindReport")' + '?state=' + state_id,
            contentType: "application/json; charset=utf-8",
            datatype: "html",
            success: function (result, status, xhr) {
               
                var options = "";
                for (var j = 0; j < result.length; j++) {
                    options += '<option value="' + result[j].spv + '">' + result[j].spv + '</option>';
                }
                $("#spvY").multipleSelect('destroy');

                $("#spvY").html(options);
                $('#spvY').multipleSelect({
                    "minimumCountSelected": 2,
                    "placeholder": "Select SPV(s)",
                });
            }
        });
        getSiteY();
    }
    function GetSPVM() {
        let state_id = "";
        if($('#stateM').val())
        {
            $('select#stateM option:selected').each(function () {
                state_id += "'" + $(this).val() + "',";
            });
            state_id = state_id != "" ? state_id.slice(0, -1) : state_id;
        }
        $.ajax({
            type: "GET",
            url: '@Url.Action("GetSPVList", "WindReport")' + '?state=' + state_id,
            contentType: "application/json; charset=utf-8",
            datatype: "html",
            success: function (result, status, xhr) {
               
                var options = "";
                for (var j = 0; j < result.length; j++) {
                    options += '<option value="' + result[j].spv + '">' + result[j].spv + '</option>';
                }
                $("#spvM").multipleSelect('destroy');
                
                $("#spvM").html(options);
                $('#spvM').multipleSelect({
                    "minimumCountSelected": 2,
                    "placeholder": "Select SPV(s)",
                });
            }
        });
        getSiteM();
    }
    function GetSPV() {
        let state_id = "";
        if($('#state').val())
        {
            $('select#state option:selected').each(function () {
                state_id += "'"+$(this).val() + "',";
            });
            state_id = state_id != "" ? state_id.slice(0, -1) : state_id;
        }
        $.ajax({
            type: "GET",
            ///url: "/WindReport/GetSPVList?state=" + val,
            url: '@Url.Action("GetSPVList", "WindReport")' + '?state=' + state_id,
            contentType: "application/json; charset=utf-8",
            datatype: "html",
            success: function (result, status, xhr) {
           
                var options = "";
                for (var j = 0; j < result.length; j++) {
                    options += '<option value="' + result[j].spv + '">' + result[j].spv + '</option>';
                }
                $("#spv").multipleSelect('destroy');
                $("#spv").html(options);
                $('#spv').multipleSelect({
                    "minimumCountSelected": 2,
                    "placeholder": "Select SPV(s)",
                });

            }
        });
        getSite();
    }
    function getSite() {
        let state_id = "";
        $('select#state option:selected').each(function () {
            state_id += $(this).val() + ",";
        });
        state_id = state_id != "" ? state_id.slice(0, -1) : state_id;

        let spv = "";
        $('select#spv option:selected').each(function () {
            spv += $(this).val() + ",";
        });
        spv = spv != "" ? spv.slice(0, -1) : spv;
       
        $.ajax({
            type: "POST",
            url: '@Url.Action("GetSiteList", "WindReport")' + '?state=' + state_id + '&spv=' + spv,
            contentType: "application/json; charset=utf-8",
            datatype: "html",
            success: function (result, status, xhr) {
                var options = "";
                for (var k = 0; k < result.length; k++) {
                    options += '<option value="' + result[k].site_master_id + '">' + result[k].site + '</option>';
                }
                $("#site").multipleSelect('destroy');
                $("#site").html("").html(options);

                $('#site').multipleSelect({
                    "minimumCountSelected": 2,
                    "placeholder": "Select Site(s)",
                });
