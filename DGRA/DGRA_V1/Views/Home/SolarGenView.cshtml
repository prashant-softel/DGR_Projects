<script type="text/javascript">
    $(document).ready(function () {
        $('#daterange').daterangepicker(
            {
                locale: {
                    format: 'DD/MM/YYYY',
                }
            });
    });

    function getReport() {

        var daterange = $('#daterange').val();
        var arr1 = new Array();
        arr1 = daterange.split("-");
        var fromDate = moment(arr1[0], 'DD/MM/YYYY').format('YYYY-MM-DD');
        var toDate = moment(arr1[1], 'DD/MM/YYYY').format('YYYY-MM-DD');

        $.ajax({
            type: "GET",
            url: "/SolarView/SolarGenView?fromDate=" + fromDate + "&toDate=" + toDate,
            contentType: "application/json; charset=utf-8",
            //data: '{"countryname":"' + selcountry + '"}',
            datatype: "JSON",
            success: function (result, status, xhr) {
                console.log(result);
                var tbl = '<table id="example1" class="table table-bordered table-striped">';
                tbl += '<thead><tr>';
                tbl += '<td class="theaddata" style="width:10%">Date</td>';
                tbl += '<td class="theaddata" style="width:4%">State</td>';
                tbl += '<td class="theaddata" style="width:6%">Site</td>';
                tbl += '<td class="theaddata" style="width:5%">Location Name</td>';
                tbl += '<td class="theaddata" style="width:5%">GHI</th>';
                tbl += '<td class="theaddata" style="width:8%">POA</td>';
                tbl += '<td class="theaddata" style="width:8%">Expected kWh</td>';
                tbl += '<td class="theaddata" style="width:4%">INV kWh</td>';
                tbl += '<td class="theaddata" style="width:4%">Plant kWh</td>';
                tbl += '<td class="theaddata" style="width:4%">INV PR</td>';
                tbl += '<td class="theaddata" style="width:6%">Plant PR</td>';
                tbl += '<td class="theaddata" style="width:6%">MA</td>';
                tbl += '<td class="theaddata" style="width:6%">IGA</td>';
                tbl += '<td class="theaddata" style="width:5%">EGA</td>';
                tbl += '<td class="theaddata" style="width:5%">INV PLF AC</td>';
                tbl += '<td class="theaddata"style="width:6%">INV PLF DC</td>';
                tbl += '<td class="theaddata" style="width:5%">Plant PLF AC</td>';
                tbl += '<td class="theaddata" style="width:5%">Plant PLF DC</td>';
                tbl += '</tr></thead>';

                if (result.length > 0) {
                    for (var i = 0; i < result.length; i++) {

                        //var tempDate = new Date(result[i].date);
                        //var formattedDate = [tempDate.getDate(), tempDate.getMonth() + 1, tempDate.getFullYear()].join('-');
                        var formattedDate = moment(result[i].date, 'YYYY/MM/DD').format('DD-MM-YYYY');
                        tbl += '<tr>';
                        tbl += '<td class="tbodydata">' + formattedDate + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].state + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].site + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].location_name + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].ghi + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].poa+ '</td>';
                        tbl += '<td class="tbodydata">' + result[i].expected_kwh+ '</td>';
                        tbl += '<td class="tbodydata">' + result[i].inv_kwh + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].plant_kwh + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].inv_pr + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].plant_pr + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].ma + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].iga + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].ega + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].inv_plf_ac + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].inv_plf_dc + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].plant_plf_ac + '</td>';
                        tbl += '<td class="tbodydata">' + result[i].plant_plf_dc + '</td>';
                        tbl += '</tr>';
                    }
                }
                else {

                    // tbl += '<tr><th colspan="19" style="text-align:center">Data Not Available <th></tr>';
                }
                tbl += '</table>';
                $("#DailyGenView").html(tbl);
                /*$('#example1').DataTable({
                    "paging": true,
                    "lengthChange": false,
                    "searching": false,
                    "ordering": true,
                    "info": true,
                    "autoWidth": false,
                    "responsive": true,
                    "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
                }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');*/
                //$("#example1").DataTable({
                //    "responsive": true, "lengthChange": false, "autoWidth": false,
                //   // buttons: [
                //      //  'copy', 'excel', 'pdf'
                //    //]
                //});
            }
