<script type="text/javascript">

    window.onload = function () {
        $.ajax({
            type: "GET",
            //url: "/SolarReport/GetFinancialYear",
            url: '@Url.Action("GetFinancialYear", "SolarReport")',
            contentType: "application/json; charset=utf-8",
            //data: '{"countryname":"' + selcountry + '"}',
            datatype: "html",
            success: function (result, status, xhr) {
                var options = "";
                options += '<option value="">Select Financial Year</option>';
                for (var i = 0; i < result.length; i++) {
                    options += '<option value="' + result[i].financial_year + '">' + result[i].financial_year + '</option>';
                }
                $("#fy").html(options);
            }
        });
    }
    function getReport() {
        var year = $('select#fy option:selected').val();
        var month = $('select#month option:selected').val();
        $.ajax({
            type: "GET",
            url: '@Url.Action("SolarMonthlyJMRView", "SolarView")' + '?year=' + year + '&month=' + month,
            //url: "/SolarView/SolarMonthlyJMRView?year=" + year + "&month=" + month,
            contentType: "application/json; charset=utf-8",
            //data: '{"countryname":"' + selcountry + '"}',
            datatype: "JSON",
            success: function (result, status, xhr) {
                console.log(result);
                var tbl = '<table id="example1" class="table table-bordered table-striped dataTable dtr-inline">';
                tbl += '<thead  class="tbl-head"><tr>';
                tbl += '<th>FY</th>';
                tbl += '<th>Site</th>';
                tbl += '<th>Plant Selection</th>';
                tbl += '<th>Controller KWH/INV KWH</th>';
                tbl += '<th>Scheduled Units (kWh)</th>';
                tbl += '<th>Export kWh</th>';
                tbl += '<th>Import kWh</th>';
                tbl += '<th>Net Export kWh</th>';
                tbl += '<th>Export kVAh</th>';
                tbl += '<th>Import kVAh</th>';
                tbl += '<th>Export kVArh lag</th>';
                tbl += '<th>Import kVArh lag</th>';
                tbl += '<th>Export kVArh lead</th>';
                tbl += '<th>Import kVArh lead</th>';
                tbl += '<th>JMR Date</th>';
                tbl += '<th>JMR Month</th>';
                tbl += '<th>JMR Year</th>';
                tbl += '<th>Line Loss</th>';
                tbl += '<th>Line Loss %</th>';
                tbl += '<th>RKVH %</th>';
                tbl += '</tr></thead>';

                if (result.length > 0) {
                    for (var i = 0; i < result.length; i++) {
                        var tempDate = new Date(result[i].jmR_date);
                        var formattedDate = [tempDate.getDate(), tempDate.getMonth() + 1, tempDate.getFullYear()].join('-');
                        tbl += '<tr>';
                        tbl += '<td>' + result[i].fy + '</td>';
                        tbl += '<td>' + result[i].site + '</td>';
                        tbl += '<td>' + result[i].plant_Section + '</td>';
                        tbl += '<td>' + result[i].controller_KWH_INV + '</td>';
                        tbl += '<td>' + result[i].scheduled_Units_kWh + '</td>';
                        tbl += '<td>' + result[i].export_kWh + '</td>';
                        tbl += '<td>' + result[i].import_kWh + '</td>';
                        tbl += '<td>' + result[i].net_Export_kWh + '</td>';
                        tbl += '<td>' + result[i].export_kVAh + '</td>';
                        tbl += '<td>' + result[i].import_kVAh + '</td>';
                        tbl += '<td>' + result[i].export_kVArh_lag + '</td>';
                        tbl += '<td>' + result[i].import_kVArh_lag + '</td>';
                        tbl += '<td>' + result[i].export_kVArh_lead + '</td>';
                        tbl += '<td>' + result[i].import_kVArh_lead + '</td>';
                        tbl += '<td>' + formattedDate + '</td>';
                        tbl += '<td>' + result[i].jmR_Month + '</td>';
                        tbl += '<td>' + result[i].jmR_Year + '</td>';
                        tbl += '<td>' + result[i].lineLoss + '</td>';
                        tbl += '<td>' + result[i].line_Loss_percentage + '</td>';
                        tbl += '<td>' + result[i].rkvH_percentage + '</td>';
                        tbl += '</tr>';
                    }
                }
                else {

                    tbl += '<tr><th colspan="20" style="text-align:center">Data Not Available <th></tr>';
                }
                tbl += '</table>';
                $("#MontlyJMRView").html(tbl);

            }
        });
    }
</script>