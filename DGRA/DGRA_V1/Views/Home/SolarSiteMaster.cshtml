<script type="text/javascript">
  
    window.onload = function () {
      
        getSiteData();
    }
    function getSiteData() {
    $.ajax({
        type: "GET",
        url: "/SolarReport/GetSolarSiteMaster",
        contentType: "application/json; charset=utf-8",
        //data: '{"countryname":"' + selcountry + '"}',
        datatype: "html",
        success: function (result, status, xhr) {
           
            console.log(result);
            var tbl = '<table id="example1" class="table table-bordered table-striped">';
            tbl += '<thead class="tbl-head"><tr>';
            tbl += '<td>Country</td>';
            tbl += '<td>State</td>';
            tbl += '<td>SPV</td>';
            tbl += '<td>Site</td>';
            tbl += '<td>DC Capacity(MWp)</td>';
            tbl += '<td>AC Capacity(MWp)</td>';
            tbl += '<td>Total Tariff</td>';
            tbl += '</tr></thead>';
            if (result.length > 0) {
                for (var i = 0; i < result.length; i++) {

                    tbl += '<tr>';
                    tbl += '<td>' + result[i].country + '</td>';
                    tbl += '<td>' + result[i].state + '</td>';
                    tbl += '<td>' + result[i].spv + '</td>';
                    tbl += '<td>' + result[i].site + '</td>';
                    tbl += '<td>' + result[i].dc_capacity + '</td>';
                    tbl += '<td>' + result[i].ac_capacity+ '</td>';
                    tbl += '<td>' + result[i].total_tarrif + '</td>';
                    tbl += '</tr>';
                }
            }
            else {

                // tbl += '<tr><th colspan="19" style="text-align:center">Data Not Available <th></tr>';
            }
            tbl += '</table>';
            $("#siteData").html(tbl);
            $.noConflict();
            $('#example1').DataTable({
                "responsive": true, "lengthChange": false, "autoWidth": false,
                "buttons": ["csv", "excel","colvis"],
                "paging": true,
                "lengthChange": false,
                "searching": true,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
        }
    });
    }
   
   

</script>
